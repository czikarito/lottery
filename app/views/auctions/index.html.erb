<% if current_user %>
    <div class="panel panel-default">
      <div class="panel-body">
        <h2>You won items:</h2>
        <ul class="demo-list-item mdl-list">
          <% won_items.each do |item| %>
              <li class="mdl-list__item">
                <%= item.name %>
              </li>
          <% end %>
        </ul>
      </div>
    </div>



    <div class="panel panel-default">
      <div class="panel-body">
        <h2>You lost items:</h2>
        <ul class="demo-list-item mdl-list">
          <% lost_items.each do |item| %>
              <li class="mdl-list__item">
                <%= item.name %>
              </li>
          <% end %>
        </ul>
      </div>
    </div>



<% end %>
